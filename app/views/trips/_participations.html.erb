<OL>
<% @participations.each { |p| %>
<LI><B>
<%= p.name %>
<% @user = User.find(p.user_id) %>
<%= @user.first_name %> <%= @user.last_name %>
(<%= @user.email %>)</B>
<% if p.user_id == current_user().id %>
<I><% @profile= Profile.find(p.profile_id) %></I>
<%= @profile.name %>
<% end %>
Travel Date: <%= p.traveldate %>
</LI>
<% } %>
</OL>

