<h1>New user</h1>


<% form_for(@user) do |f| %>
  <%= f.error_messages %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br/>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br/>
  <%= f.label :login %>
  <%= f.text_field :login %><br />
  <%= render :partial => '/users/form', :object => f %>
  <%= f.submit 'Sign up', :disable_with => 'Please wait...' %>

<% end %>
